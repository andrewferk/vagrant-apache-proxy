<IfModule mod_proxy.c>

	ProxyRequests Off
	ProxyVia On

	<Proxy *>
		AddDefaultCharset off
		Order deny,allow
		Deny from all
	</Proxy>

	ProxyPass /ad/ http://www.ackmanndickenson.com/
	ProxyPassReverse /ad/ http://www.ackmanndickenson.com/
	<Proxy http://www.ackmanndickenson.com/
		Order Allow,Deny
		Allow from all
	</Proxy>
</IfModule>
